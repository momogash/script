colorscheme slate "changes the colorscheme for vim
set number "line numbers
syntax on "turns the syntax on
set showcmd "shows the last typed command
set cursorline "line marker for readability
set wildmenu "autocomplete for command menu
set showmatch "high lights matching paranthesis
set backup
set backupdir=~/.vim-tmp,~/.tmp.~/tmp,/var/tmp,/tmp
set backupskip=/tmp/*,/private/tmp*
set directory=~/.vim-tmp,~/.tmp,~/tmp,/var/tmp,/tmp
